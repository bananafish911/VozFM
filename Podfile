# Uncomment this line to define a global platform for your project
platform :ios, '9.0'
# Uncomment this line if you're using Swift
use_frameworks!

source 'https://github.com/CocoaPods/Specs.git'

target 'VozFM' do
    
    #UI
    #pod 'Spring', '~> 1.0'	#animations
    pod 'KRProgressHUD' 	#progress HUD
    
    #Networking
    pod 'Alamofire', '~> 4.0'
    pod 'Kingfisher'        #library for downloading and caching images from the web
    pod 'SwiftyJSON'
    
    #Utilites
    pod 'Smooch'            #feedback module
    pod 'Fabric' 			#awesome crash reporting
	pod 'Crashlytics'		#awesome crash reporting

    # RX
    pod 'RxSwift', '~> 3.2'
    pod 'RxCocoa', '~> 3.2'

end
